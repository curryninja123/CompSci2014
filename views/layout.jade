doctype html
html
  head
    title= title
    link(rel='stylesheet', href='/stylesheets/style.css')
  body
    #nav
        ul
            if !loggedIn
                li
                    a(href="/users/login") Log In
            else
                if isAdmin
                    li
                        a(href="/logic/admin") Admin Panel
                else
                    li
                        a(href="/logic/general/edituser") Edit Team Member Names
                li
                    a(href="/users/logout") Log Out
            li
                a(href="/info") Info
            li
                a(href="#") Sign Up
    for message in error_messages
        p.flash.error= message
    for message in success_messages
        p.flash.success= message
    block content